  langid = {english},
  keywords = {Bioinformatics,Cell signalling,Cellular signalling networks,Computational biology and bioinformatics,Statistical methods},
  file = {/Users/memons/Zotero/storage/77U2QX8Y/s41467-023-39608-w.pdf}
}

