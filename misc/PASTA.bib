@article{aliGraphCompassSpatialMetrics2024,
  title = {{{GraphCompass}}: Spatial Metrics for Differential Analyses of Cell Organization across Conditions},
  shorttitle = {{{GraphCompass}}},
  author = {Ali, Mayar and Kuijs, Merel and {Hediyeh-zadeh}, Soroor and Treis, Tim and Hrovatin, Karin and Palla, Giovanni and Schaar, Anna C and Theis, Fabian J},
