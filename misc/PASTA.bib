@article{banerjeeNearestNeighbourDistributions2021,
  title = {Nearest Neighbour Distributions: {{New}} Statistical Measures for Cosmological Clustering},
  shorttitle = {Nearest Neighbour Distributions},
  author = {Banerjee, Arka and Abel, Tom},
  year = {2021},
  month = feb,
  journal = {Monthly Notices of the Royal Astronomical Society},
  volume = {500},
  number = {4},
  pages = {5479--5499},
