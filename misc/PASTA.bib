@article{vuFunSpaceFunctionalSpatial2023,
  title = {{{FunSpace}}: {{A}} Functional and Spatial Analytic Approach to Cell Imaging Data Using Entropy Measures},
  shorttitle = {{{FunSpace}}},
  author = {Vu, Thao and Seal, Souvik and Ghosh, Tusharkanti and Ahmadian, Mansooreh and Wrobel, Julia and Ghosh, Debashis},
  year = {2023},
  month = sep,
  journal = {PLOS Computational Biology},
  volume = {19},
